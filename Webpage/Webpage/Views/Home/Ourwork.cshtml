
@{
    ViewBag.Title = "Ourwork";
}
<section class="row">
    <div class="img-container">
        <img class="img-responsive" src="~/images/SlideShow/development.jpg" />
        <h7 class="homeImageLink">
            <span>We help your company develop</span>
        </h7>
    </div>
</section>
<div class="info-tiles">
    <div class="info-tile-row">
        <div class="info-tile" style="background-color:white;">
            <a href="@Url.Action("Career","Home/Career")">
                <img class="tile-img-dt" src="~/images/Ourwork/wallpaper-widescreen-car-tech-197362.jpg" />
                <img class="tile-img-mb" src="~/images/Ourwork/wallpaper-widescreen-car-tech-197362.jpg" />

                <div class="info-tile-info">
                    <div class="info-tile-info-inner">
                        <p class="top-text">Start your</p><h3>CAREER</h3>
                        <p class="bottom-text">Start a new adventure</p>
                    </div>
                </div>
            </a>
        </div>
        <div class="info-tile" style="background-color:white;">
            <a href="@Url.Action("Career","Home/Expertise")">

                <img class="tile-img-dt" src="~/images/Ourwork/myfordtouch-sg.jpg" />
                <img class="tile-img-mb" src="~/images/Ourwork/myfordtouch-sg.jpg" />

                <div class="info-tile-info">
                    <div class="info-tile-info-inner">
                        <p class="top-text">Look we can</p><h3>INNOVATE</h3>
                        <p class="bottom-text">What you need</p>
                    </div>
                </div>
            </a>
        </div>
    </div>

</div>


